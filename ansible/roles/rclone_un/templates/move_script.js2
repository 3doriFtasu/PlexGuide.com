#!/bin/bash
sleep 30
while true
do
## Sync, Sleep 8 Minutes, Repeat. BWLIMIT Prevents Google 750GB Google Upload Ban
rclone move --bwlimit 9M --tpslimit 4 --exclude '*.partial~' --exclude="**_HIDDEN" --exclude=".unionfs/**" --no-traverse --checkers=16 --max-size 99G --log-level INFO --stats 15s /mnt/move gdrive:/
sleep 480
done