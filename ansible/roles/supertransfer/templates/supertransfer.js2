[Unit]
Description=Time
After=multi-user.target

[Service]
User=0
Group=0
Type=simple
ExecStart=/bin/bash /opt/plexguide/scripts/docker-no/supertransfer.sh
ExecStop=ps -ef | grep supertransfer | grep -v  "grep"  | awk '{print $2}' | xargs kill;
KillMode=process

[Install]
WantedBy=multi-user.target
