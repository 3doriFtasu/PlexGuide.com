#!/bin/bash
############# https://github.com/plexguide/PlexGuide.com/graphs/contributors ###
drives_menu_start() {

drives_menu_start_setup

find /pg/transport -type f -name '*.' | xargs stat -f '%c %N' | sort

find /pg/transport -type f -printf "\n%AT %p" | awk '{print $2}' | tail -n +2



rclone moveto "$upload_file" "/pg/transfer/$folder_num/" \
--exclude="**_HIDDEN~" --exclude=".unionfs/**" \
--exclude="**partial~" --exclude=".unionfs-fuse/**" \
--exclude=".fuse_hidden**" --exclude="**.grab/**" \

}
