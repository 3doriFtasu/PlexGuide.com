drives_deploy() {

drives_menu_start_setup

#common_header "âŒ› DEPLOYMENT: Service Cleanup & Stopping Docker Containers"; sleep 2
ansible-playbook /pg/mods/drives/ymls/remove.yml

if [[ "$gdtokeninfo" == "VALID" ]]; then
driveselect=gd
drives_deploy_mounts
fi
if [[ "$gdtokeninfo" == "VALID" && "$gcryptcheck" == "YES" ]]; then drives_deploy_mounts gc; fi

drives_menu_start && exit
}
