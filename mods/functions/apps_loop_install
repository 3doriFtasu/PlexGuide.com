apps_loop_install() {

  i=0
  while read p; do

    let "i=i+1"
    ansible-playbook "/pg/mods/apps/$app_select/$p.yml"

  done </pg/tmp/appstoinstall.list

echo ""
read -p "↘️  Installed $i $app_select application(s) | Press [ENTER]" typed < /dev/tty

# recycle menu back to user
apps_menu; exit

}
