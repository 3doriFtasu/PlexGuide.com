#!/bin/bash
############# https://github.com/plexguide/PlexGuide.com/graphs/contributors ###
apps_install() {

# forces function refresh of function output ###################################
bash /pg/mods/functions/.primary_apps.sh

container_rebuild $1
ansible-playbook "/pg/apps/$1.yml"

rm -rf "/pg/data/$1/apply.settings"
echo ""
read -p "↘️  Installed $1 | Press [Enter]" typed < /dev/tty

}
