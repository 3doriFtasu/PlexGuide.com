apps_menu_select() {

## generate apps
apps_gen_lists

## remove prior pending apps to install list
rm -rf /pg/tmp/appstoinstall.list

case $typed in
    1 )
      app_select="primary"
      apps_interface ;;
    2 )
      app_select="community"
      apps_interface ;;
    3 )
      app_select="core"
      apps_interface ;;
    z )
      ;;
    Z )
      ;;
    * )
      apps_menu ;;
esac

}
