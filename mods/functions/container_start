#!/bin/bash
############# https://github.com/plexguide/PlexGuide.com/graphs/contributors ###
container_start () {

setimage=$(cat "/pg/data/$pgrole/image.set")

container_start="
    - name: 'Deploying $pgrole'
      docker_container:
        name: '$pgrole'
        image: '$setimage'
        pull: yes"
}
